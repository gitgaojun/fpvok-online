	Z-INDEX: 1000; BORDER-BOTTOM: #999 1px solid; BORDER-LEFT: #999 1px solid; LINE-HEIGHT: 24px; BACKGROUND: #ffffff; HEIGHT: 24px; COLOR: #990000; BORDER-TOP: #999 1px solid; BORDER-RIGHT: #999 1px
}
.ddsmoothmenu-v #chcss A:hover {
	BORDER-BOTTOM: #999 1px solid; BORDER-LEFT: #999 1px solid; LINE-HEIGHT: 24px; BACKGROUND: #ffffff; HEIGHT: 24px; COLOR: #990000; BORDER-TOP: #999 1px solid; BORDER-RIGHT: #999 1px
}
.ddsmoothmenu-v #chcss #chcss A:hover {
	BORDER-BOTTOM: #999 1px solid; BORDER-LEFT: #999 1px solid; LINE-HEIGHT: 24px; BACKGROUND: #ffffff; HEIGHT: 24px; COLOR: #990000; BORDER-TOP: #999 1px solid; BORDER-RIGHT: #999 1px
}
.ddsmoothmenu-v #chcss UL LI A:hover {
	BORDER-BOTTOM-STYLE: none; LINE-HEIGHT: 24px; BORDER-RIGHT-STYLE: none; BORDER-TOP-STYLE: none; BACKGROUND: #ffffff; HEIGHT: 24px; COLOR: #990000; BORDER-LEFT-STYLE: none
}
.ddsmoothmenu-v #chcss #chcss UL LI A:hover {
	BORDER-BOTTOM-STYLE: none; LINE-HEIGHT: 24px; BORDER-RIGHT-STYLE: none; BORDER-TOP-STYLE: none; BACKGROUND: #ffffff; HEIGHT: 24px; COLOR: #990000; BORDER-LEFT-STYLE: none
}
